FoamFile
{
    format      ascii;
    class       volScalarField;
    object      nuSgs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    Mammut
    {
        type	        nutUSpaldingWallFunction;
        value           $internalField;
    }

    wand
    {
        type	        nutUSpaldingWallFunction;
        value           $internalField;
    }

    boden
    {
        type	        nutUSpaldingWallFunction;
        value           $internalField;
    }

    oben
    {
        type	        slip;
    }

}

