FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    Mammut
    {
        type MRFnoSlip;
        MRFZoneName zone1;
    }

    wand
    {
        type	        fixedValue;
        value           uniform (0 0 0);
    }

    boden
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    oben
    {
        type            slip;
    }

}

